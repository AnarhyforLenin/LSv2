<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:background="@drawable/back1"
    android:layout_height="match_parent"
    tools:context=".Start2">

    <ImageView
        android:id="@+id/w1n"
        android:layout_width="160dp"
        android:layout_height="200dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="500dp"
        android:scaleType="centerCrop"
        android:src="@drawable/w1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/bed1"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_marginEnd="160dp"
        android:layout_marginBottom="212dp"
        android:scaleType="centerCrop"
        android:src="@drawable/bed1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/car1"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_marginEnd="76dp"
        android:layout_marginBottom="32dp"
        android:rotation="90"
        android:scaleType="centerCrop"
        android:scaleX="-1"
        android:src="@drawable/car1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/plant1"
        android:layout_width="160dp"
        android:layout_height="180dp"
        android:layout_marginBottom="248dp"
        android:scaleType="centerCrop"
        android:scaleX="-1"
        android:src="@drawable/plant1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="12dp"
        android:fontFamily="@font/fontone"
        android:text="Happiness"
        android:textColor="@color/Green"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/vertical_progressbar1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ProgressBar
        android:id="@+id/vertical_progressbar1"
        style="@style/Widget.ProgressBar.Vertical"
        android:layout_width="25dp"
        android:layout_height="120dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="336dp"
        android:max="100"
        android:min="0"
        android:progress="100"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView6" />

    <TextView
        android:id="@+id/health"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="42dp"
        android:autoSizeMaxTextSize="25dp"
        android:autoSizeMinTextSize="12dp"
        android:autoSizeStepGranularity="2dp"
        android:autoSizeTextType="uniform"
        android:fontFamily="@font/fontone"
        android:gravity="center_horizontal"
        android:text="100"
        android:textColor="@color/Green"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vertical_progressbar1" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="90dp"
        android:layout_height="30dp"
        android:layout_marginStart="8dp"
        android:fontFamily="@font/fontone"
        android:gravity="center_horizontal"
        android:text="Eat"
        android:textColor="@color/Green"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="parent" />

    <ProgressBar
        android:id="@+id/vertical_progressbar2"
        style="@style/Widget.ProgressBar.Vertical2"
        android:layout_width="25dp"
        android:layout_height="120dp"
        android:layout_marginTop="28dp"
        android:max="100"
        android:progress="100"
        app:layout_constraintEnd_toStartOf="@+id/vertical_progressbar3"
        app:layout_constraintStart_toEndOf="@+id/vertical_progressbar1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/eat"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="146dp"
        android:autoSizeMaxTextSize="25dp"
        android:autoSizeMinTextSize="10dp"
        android:autoSizeStepGranularity="2dp"
        android:autoSizeTextType="uniform"
        android:fontFamily="@font/fontone"
        android:gravity="center_horizontal"
        android:text="100"
        android:textColor="@color/Green"
        android:textSize="25dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vertical_progressbar2" />

    <TextView
        android:layout_width="90dp"
        android:layout_height="30dp"
        android:layout_marginStart="12dp"
        android:fontFamily="@font/fontone"
        android:gravity="center_horizontal"
        android:text="Sleep"
        android:textColor="@color/Green"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="parent" />

    <ProgressBar
        android:id="@+id/vertical_progressbar3"
        style="@style/Widget.ProgressBar.Vertical3"
        android:layout_width="25dp"
        android:layout_height="120dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="128dp"
        android:max="100"
        android:min="0"
        android:progress="100"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/happyness"
        android:layout_width="40dp"
        android:layout_height="40dp"

        android:layout_marginStart="250dp"
        android:layout_marginTop="0dp"
        android:autoSizeMaxTextSize="25dp"
        android:autoSizeMinTextSize="10dp"
        android:autoSizeStepGranularity="2dp"
        android:autoSizeTextType="uniform"
        android:fontFamily="@font/fontone"
        android:gravity="center_horizontal"
        android:text="100"
        android:textColor="@color/Green"
        android:textSize="25dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vertical_progressbar3" />

    <ImageButton

            android:id="@+id/tools_icon"
            android:layout_width="80dp"
            android:layout_height="70dp"
            android:layout_weight="1"
            android:backgroundTint="@android:color/transparent"
            android:scaleType="fitXY"
            android:src="@drawable/home3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    <ImageButton

        android:id="@+id/tools_store"
        android:layout_width="80dp"
        android:layout_height="70dp"
        android:layout_weight="1"
        android:backgroundTint="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/store3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tools_icon" />


    <ImageButton
        android:id="@+id/karambola"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="92dp"
        android:background="@drawable/happy"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
    <!--android:backgroundTint="#33FF0000"-->






</androidx.constraintlayout.widget.ConstraintLayout>